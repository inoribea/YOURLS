{
    "version": 2,
    "builds": [
      {
      "src": "admin/index.php", 
      "use": "@vercel/node",    
      "config": {
        "runtime": "php"        
      } 
    },
    {
        "src": "./build.sh",  
        "use": "@vercel/static"
    } 
    ],
    "routes": [
      {
        "src": "/user/(.*)",       
        "dest": "user/index.html"  
      },
      
      {
        "src": "/admin/(.*)",      
        "dest": "admin/index.php" 
      } 
    ]
  }
  