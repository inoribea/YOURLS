{
    "version": 2,
    "builds": [
      {
      "src": "admin/index.php", 
      "use": "@vercel/node",    
      "config": {
        "runtime": "php"        
      }
    }
    ],
    "routes": [
      {
        "src": "/user/(.*)",       
        "dest": "user/index.html"  
      },
      
      {
        "src": "/admin/(.*)",      
        "dest": "admin/index.php" 
      } 
    ]
  }
  